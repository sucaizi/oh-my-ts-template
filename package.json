{
    "name": "oh-my-ts-template",
    "version": "1.0.0",
    "description": "typecript lib/tool template",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sucaizi/oh-my-ts-template.git"
    },
    "main": "dist/index.js",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "build": "rimraf dist types && gulp",
        "lint": "eslint src",
        "prettier": "prettier src test --write",
        "changelog": "rimraf CHANGELOG.md && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
    },
    "keywords": [
        "typecript",
        "template"
    ],
    "author": "sucaizi",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^9.1.1",
        "@commitlint/config-conventional": "^9.1.1",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "conventional-changelog-cli": "^2.1.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "gulp": "^4.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "husky": "^4.2.5",
        "merge2": "^1.4.1",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "typescript": "^3.9.7"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
